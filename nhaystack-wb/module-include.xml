<types>
  <!--nhaystack.ui-->
  <type class="nhaystack.ui.BEquipRefFE" name="EquipRefFE"/>
  <type class="nhaystack.ui.BGroupPane" name="GroupPane"/>
  <type class="nhaystack.ui.BHDictDialog" name="HDictDialog"/>
  <type class="nhaystack.ui.BHDictEditor" name="HDictEditor"/>
  <type class="nhaystack.ui.BHDictEditorGroup" name="HDictEditorGroup"/>
  <type class="nhaystack.ui.BHDictFE" name="HDictFE">
    <agent>
      <on type="nhaystack:HDict"/>
    </agent>
  </type>
  <type class="nhaystack.ui.BHFloorFE" name="HFloorFE">
    <agent>
      <on type="nhaystack:HFloor"/>
    </agent>
  </type>
  <type class="nhaystack.ui.BHNumFE" name="HNumFE">
    <agent>
      <on type="nhaystack:HNum"/>
    </agent>
  </type>
  <type class="nhaystack.ui.BHTagsDialog" name="HTagsDialog"/>
  <type class="nhaystack.ui.BHTagsFE" name="HTagsFE">
    <agent>
      <on type="nhaystack:HTags"/>
    </agent>
  </type>
  <type class="nhaystack.ui.BHTimeZoneFE" name="HTimeZoneFE">
    <agent>
      <on type="nhaystack:HTimeZone"/>
    </agent>
  </type>
  <type class="nhaystack.ui.BHUnitFE" name="HUnitFE">
    <agent>
      <on type="nhaystack:HUnit"/>
    </agent>
  </type>
  <type class="nhaystack.ui.BMarkerSet" name="MarkerSet"/>
  <type class="nhaystack.ui.BSiteRefFE" name="SiteRefFE"/>
  <type class="nhaystack.ui.BHCoordFE" name="HCoordFE"/>
  <!--nhaystack.ui.history-->
  <type class="nhaystack.ui.history.BNHaystackHistoryImportManager" name="NHaystackHistoryImportManager">
    <agent>
      <on type="nhaystack:NHaystackHistoryDeviceExt"/>
    </agent>
  </type>
  <!--nhaystack.ui.point-->
  <type class="nhaystack.ui.point.BNHaystackPointManager" name="NHaystackPointManager">
    <agent>
      <on type="nhaystack:NHaystackPointDeviceExt"/>
      <on type="nhaystack:NHaystackPointFolder"/>
    </agent>
  </type>
  <!--nhaystack.ui.view-->
  <type class="nhaystack.ui.view.BAddHaystackSlot" name="AddHaystackSlot"/>
  <type class="nhaystack.ui.view.BNHaystackQueryView" name="NHaystackQueryView">
    <agent>
      <on type="nhaystack:NHaystackService"/>
    </agent>
  </type>
  <type class="nhaystack.ui.view.BNHaystackServiceView" name="NHaystackServiceView">
    <agent>
      <on type="nhaystack:NHaystackService"/>
    </agent>
  </type>
</types>